{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ClientApp": "http://localhost:3000",
  "ReverseProxy": {
    "Routes": {
      "ExpertsRead" : {
        "ClusterId": "Experts",
        "Match": {
          "Path": "/Experts/{**catch-all}",
          "Methods": ["GET"]
        },
        "Transforms": [
          {
            "PathPattern": "api/Experts/{**catch-all}"
          }
        ]
      },
      "ExpertsWrite" : {
        "ClusterId": "Experts",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/Experts/{**catch-all}",
          "Methods": ["POST", "PUT", "DELETE"]
        },
        "Transforms": [
          {
            "PathPattern": "api/Experts/{**catch-all}"
          }
        ]
      },
      "search": {
        "ClusterId": "search",
        "Match": {
          "Path": "/search/{**catch-all}",
          "Methods": ["GET"]
        },
        "Transforms": [
          {
            "PathPattern": "api/search/{**catch-all}"
          }
        ]
      },
      "SelectingWrite": {
        "ClusterId": "Selecting",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/Selecting",
          "Methods": ["POST"]
        },
        "Transforms": [
          {
            "PathPattern": "api/Selecting"
          }
        ]
      },
      "SelectingRead": {
        "ClusterId": "Selecting",
        "Match": {
          "Path": "/Selecting/{**catch-all}",
          "Methods": ["GET"]
        },
        "Transforms": [
          {
            "PathPattern": "api/Selecting/{**catch-all}"
          }
        ]
      },
      "notifications": {
        "ClusterId": "notifications",
        "CorsPolicy": "customPolicy",
        "Match": {
          "Path": "/notifications/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "Experts": {
        "Destinations": {
          "ExpertApi": {
            "Address": "http://Expert-clusterip"
          }
        }
      },
      "search": {
        "Destinations": {
          "searchApi": {
            "Address": "http://search-clusterip"
          }
        }
      },
      "Selecting": {
        "Destinations": {
          "bidApi": {
            "Address": "http://bid-clusterip"
          }
        }
      },
      "notifications": {
        "Destinations": {
          "notifyApi": {
            "Address": "http://notify-clusterip"
          }
        }
      }
    }
  }
}
